{
  "name": "nebula-test-fragment",
  "version": "0.1.0",
  "description": "Base fragment and utilities for e2e testing",
  "license": "Apache-2.0",
  "author": "mt0erfztxt",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "ci-tests": "npm run test-node && npm run test-browser_browserstack",
    "local-tests": "npm run test-node && npm run test-browser_local",
    "test": "npm run ci-tests",
    "test-browser_browserstack": "testcafe 'browserstack:Chrome@53.0:Windows 10' test/browser",
    "test-browser_local": "testcafe chrome test/browser",
    "test-node": "mocha --require babel-core/register test/node/**"
  },
  "dependencies": {
    "app-root-path": "^2.0.1",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.6.1",
    "change-case": "^3.0.1",
    "deep-object-diff": "^1.1.0",
    "escape-string-regexp": "^1.0.5",
    "lodash": "^4.17.10",
    "testcafe": "^0.16.2",
    "testcafe-browser-provider-browserstack": "^1.4.0",
    "typeof--": "^1.3.3",
    "url-parse": "^1.1.9"
  },
  "devDependencies": {
    "mocha": "^5.1.1",
    "nyc": "^11.7.1",
    "sinon": "^5.0.3",
    "unexpected": "^10.37.5",
    "unexpected-sinon": "^10.10.0"
  }
}
