{
  "name": "nebula-test-fragment",
  "version": "0.3.0",
  "description": "Page object and utilities for testing",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/mt0erfztxt/nebula-test-fragment.git"
  },
  "author": "Sergey Kozhevnikov <mt0erfztxt@gmail.com>",
  "main": "lib",
  "bin": {
    "generate-page-objects": "./lib/cli.js"
  },
  "files": [
    "lib"
  ],
  "directories": {
    "test": "src/test"
  },
  "scripts": {
    "clean": "rm -rf lib",
    "compile": "npx babel --copy-files --extensions '.js' --ignore 'src/test/spec/**/*,src/main/generated-src' --out-dir lib src",
    "compile-watch": "npm run compile -- --watch",
    "jasmine-tests": "npx jasmine --config=jasmine.json",
    "prepare": "npm run clean && npm run compile && mv lib/main/* lib && rm -rf lib/main lib/test",
    "test": "npm run jasmine-tests && npm run testcafe-tests",
    "testcafe-tests": "npm run clean && npm run compile && npx testcafe --selector-timeout=3500 chromium lib/test/testcafe"
  },
  "dependencies": {
    "@sindresorhus/is": "1.2.0",
    "app-root-path": "2.2.1",
    "change-case": "3.1.0",
    "deep-object-diff": "1.1.0",
    "escape-string-regexp": "2.0.0",
    "glob": "7.1.6",
    "handlebars": "4.7.3",
    "mkdirp": "1.0.3",
    "testcafe": "1.8.1",
    "url-parse": "1.4.7"
  },
  "devDependencies": {
    "@babel/cli": "7.8.3",
    "@babel/core": "7.8.3",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-export-default-from": "7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "7.8.3",
    "@babel/plugin-proposal-private-methods": "7.8.3",
    "@babel/preset-env": "7.8.3",
    "@babel/register": "7.8.3",
    "@types/app-root-path": "1.2.4",
    "@types/jasmine": "3.5.1",
    "jasmine": "3.5.0",
    "jasmine-spec-reporter": "4.2.1",
    "prettier": "1.19.1",
    "unexpected": "11.12.1"
  }
}
