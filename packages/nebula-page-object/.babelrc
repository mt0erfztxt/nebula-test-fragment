{
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-export-default-from",
    "@babel/proposal-object-rest-spread",
    "@babel/proposal-private-methods",
    "@babel/transform-async-to-generator"
  ],
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "node": "8.12"
        }
      }
    ]
  ]
}
